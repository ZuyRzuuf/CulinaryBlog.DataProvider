{
  "Serilog": {
    "Using": [
      "Serilog.Sinks.Console",
    ],
    "MinimumLevel": {
      "Default": "Verbose",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning",
        "Microsoft.Hosting.Lifetime": "Verbose"
      }
    },
    "Enrich": [  ],
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "outputTemplate": "[{Timestamp:yyyy-MM-dd HH:mm:ss.fff zzz} {CorrelationId} {Level:u3}] {Username} {Message:lj}{NewLine}{Exception}"
        }
      }
    ]
  },
  "ConnectionStrings": {
    "database": "server=localhost;port=8084;userid=test;password=test",
    "schema": "server=localhost;port=8084;userid=test;password=test;database=culinary_blog_test"
  }
}
